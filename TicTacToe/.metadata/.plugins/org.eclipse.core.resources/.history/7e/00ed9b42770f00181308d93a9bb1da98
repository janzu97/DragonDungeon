
public class AnimationProcessor {
	static String [] frames= {"                        <\")\r\n" + 
			"                        ( >\\\r\n" + 
			"                         \" \\\\\r\n" + 
			"                            \\  \n","                        >\")\r\n" + 
					"                        ( >\\\r\n" + 
					"                         \" \\\\\r\n" + 
	"                            \\  \n"};
	static int frame=0;
	public static String getframe() {
		frame++;
		if(frame>frames.length-1) {
			frame=0;
		}
		return frames[frame];
	}

}
